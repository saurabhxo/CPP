#include <iostream>
#include <climits>
using namespace std;

int main() 
{
    int n,arr[20],i,j; 
    cin>>n;
    for(i=0; i<n; i++)
    cin>>arr[i];
    int min=INT_MAX,max=INT_MIN;
    for(i=0; i<n; i++)
    {
        if(arr[i]<min)
        min=arr[i];
        if(arr[i]>max)
        max=arr[i];
       
    }
    cout<<"Minima= "<<min<<endl;
    cout<<"Maxima= "<<max;
    
	return 0;
}
******************************************************************************************************
INPUT.
5            //n
7 6 9 3 -1   //arr[i]
OUTPUT.
Minima= -1
Maxima= 9
