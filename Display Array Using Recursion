#include <iostream>
using namespace std;
void prin(int arr[],int n, int i);
int main() 
{
    int n,i;
    cin>>n;
    int arr[n];
    for(i=0; i<n; i++)
    cin>>arr[i];
    i=0;
    prin(arr,n,i);
	return 0;
}
void prin(int arr[],int n, int i)
{
    if(i==n)
   return ;
    else
    cout<<arr[i]<<" ";
    prin(arr,n,i+1);
}
