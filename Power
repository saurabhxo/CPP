#include <iostream>
using namespace std;
int pow(int a, int b);
int main() 
{
    int a,b,res;
    cin>>a>>b;
    res=pow(a,b);
    cout<<res;
	return 0;
}
int pow(int a, int b)
{
    if(b==0)
    return 1;
    else if(b!=0)
    return a*pow(a,b-1);
}
*******************************************************************************************************************
Sample Input
10 2                         // a//b
Sample Output
100
