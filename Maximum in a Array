#include <iostream>
using namespace std;

int main() 
{
    int arr[20],n,i;
    cin>>n;
    int max=0;
    for(i=0; i<n; i++)
    cin>>arr[i];
     for(i=0; i<n; i++)
     {
         if(arr[i]>max)
         max=arr[i];
     }
     cout<<max;
	return 0;
}
********************************************************************************
Sample Input
5                 //n
1 10 3 11 5       //arr[i]
