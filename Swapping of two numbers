#include <iostream>
using namespace std;
void swap(int x, int y)
{
    int temp;
    temp=x;
    x=y;
    y=temp;
    cout<<"X= "<<x<<endl<<"Y= "<<y;
}
int main() 
{
    int x,y;
    cin>>x>>y;
    swap(x,y);
	return 0;
}
*************************************************************************************
This is a sample pogram for swapping of two numbers.
Sample Input.
20             //x
50             //y
Sample Output.
X= 50
Y= 20
