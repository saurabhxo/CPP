   #include <iostream>
using namespace std;

int main() 
{
    int x,y,temp;
    cin>>x>>y;
    temp=x;
    x=y;
    y=temp;
    ;
    cout<<"x="<<x<<endl<<"y="<<y;
    return 0;
}
******************************************************
This is a sample pogram of reversing of two numbers.
Sample Input
3      //x
2      //y
Sample Output.
x=2
y=3
