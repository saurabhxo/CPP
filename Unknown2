#include <iostream>
#include <cstring>
using namespace std;

int main() 
{
    char arr[10];
    cin>>arr;
    int i,j,n;
    n=strlen(arr);
    for(i=0; i<n; i++)
    {
        for(j=i+1; j<n; j++)
        
        {
            if(arr[i]==arr[j]);
            {
                arr[i]=0;
                arr[j]=0;
                
            }
        }
    }
     for(i=0; i<n; i++)
    cout<<arr[i];
	return 0;
}
