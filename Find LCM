#include <iostream>
using namespace std;
int main() 
{
    int n1,n2,a,b,l;
    cin>>n1>>n2;
    a=n1;
    b=n2;
    while(n1!=n2)
    {
        if(n1>n2)
        n1-=n2;
        else
        n2-=n1;
    }
    l=(a*b)/n1;
    cout<<"LCM= "<<l;
	return 0;
}
********************************************************************************************
Sample Input
13 52                //n1//n2
Sample Output
LCM= 52
