#include <iostream>
using namespace std;
void shift(int n)
{
    int arr[20],i,j,count=0;
    for(i=0; i<n; i++)
    cin>>arr[i];
    for(i=0; i<n; i++)
    {
        if(arr[i]==0)
        count++;
        if(arr[i]!=0)
        cout<<arr[i];
    }
    for(i=0; i<count; i++)
    cout<<"0";
}
int main() 
{
    int n;
    cin>>n;
    shift(n);
	return 0;
}
*************************************************************************************************
This is sample pogram for moving all zeroes to end of array.
Sample Input.
10                        //n
1 0 2 0 5 0 5 0 0 7       //arr[i]
Sample Output.
1255700000
