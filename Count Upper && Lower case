#include <iostream>
#include <cstring>
using namespace std;

int main() 
{
    string arr;
    getline(cin,arr);
    int n,upper=0,lower=0,i;
    n=arr.length();
    for(i=0; i<n; i++)
    {
        if(arr[i]='A' && arr[i]<='Z')
        upper++;
        else if(arr[i]='a' && arr[i]<='z')
        lower++;
    }
    cout<<"Upper= "<<upper<<endl;
    cout<<"Lower= "<<lower;
	return 0;
}
