#include <iostream>
using namespace std;
int fact(int n);
int main() 
{
    int n,mul;
    cin>>n;
    mul=fact(n);
    cout<<fact(n);
	return 0;
}
int fact(int n)
{
    if(n==0)
    return 1;
    else
    return n*fact(n-1);
    return 0;
}
**************************************************************************************************************
Sample Input
10                //n
Sample Output
3628800
