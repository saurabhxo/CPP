#include <iostream>
#include<cstring>
using namespace std;

int main() 
{
    int m, n,i=0,j,arr[20];
    cin>>n;
    while(n>0)
    {
        arr[i++]=n%2;
        n=n/2;
        }
    for(j=i-1; j>=0; j--)
    cout<<arr[j]; 
	return 0;
}
*****************************************************************************************************************************************
Sample Input
10
Sample Output
1010
